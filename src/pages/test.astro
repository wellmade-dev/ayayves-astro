---
import TestingLayout from "../layouts/TestingLayout.astro";
import HeroSection from "../components/sections/HeroSection.astro";
import CarouselSection from "../components/sections/CarouselSection.astro";
import LiveGridSection from "../components/sections/LiveGridSection.astro";
import ProductSlider from "../components/atoms/carousels/ProductSlider.astro";
import LiveEventSection from "../components/sections/LiveEventSection.astro";
---

<TestingLayout title="Aya Yves">
  <main>
    <HeroSection>
      <CarouselSection title="Support the Art" subtitle="Merch">
        <ProductSlider />
      </CarouselSection>
    </HeroSection>
    <LiveGridSection />
    <LiveEventSection />
  </main>
</TestingLayout>

<style lang="scss">
  @use "../styles/vars" as *;
</style>
